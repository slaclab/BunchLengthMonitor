record(calc, "$(P):$(R)")
{
  field(INPA, "$(SRC)")
  field(CALC, "A*0.01+273.15")
  field(SCAN, "1 second")
  field(HIHI,"$(HIHI=500)")
  field(HIGH,"$(HIGH=330)")
  field(LOW,"$(LOW=280)")
  field(LOLO,"$(LOLO=270)")
  field(HHSV,"$(HHSV=INVALID)")
  field(HSV,"$(HSV=MAJOR)")
  field(LSV,"$(LSV=MINOR)")
  field(LLSV,"$(LLSV=MAJOR)")
}
