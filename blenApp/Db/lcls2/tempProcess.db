record(calc, "$(P):$(R)")
{
  field(INPA, "$(SRC)")
  field(CALC, "A*0.01+273.15")
  field(SCAN, "1 second")
  field(HIHI,"$(HIHI=500)")
  field(HIGH,"$(HIGH=330)")
  field(LOW,"$(LOW=280)")
  field(LOLO,"$(LOLO=270)")
  field(HHSV,"$(HHSV=INVALID)")
  field(HSV,"$(HSV=MAJOR)")
  field(LSV,"$(LSV=MINOR)")
  field(LLSV,"$(LLSV=MAJOR)")
  field(EGU,"K")
}

record(calc, "$(P):$(R)_C")
{
  field(INPA, "$(P):$(R)")
  field(CALC, "A-273.15")
  field(SCAN, "1 second")
  field(HIHI,"$(HIHI=226.85)")
  field(HIGH,"$(HIGH=56.85)")
  field(LOW,"$(LOW=6.85)")
  field(LOLO,"$(LOLO=-3.15)")
  field(HHSV,"$(HHSV=INVALID)")
  field(HSV,"$(HSV=MAJOR)")
  field(LSV,"$(LSV=MINOR)")
  field(LLSV,"$(LLSV=MAJOR)")
  field(EGU,"C")
}
