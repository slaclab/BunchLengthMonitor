record(calcout, "$(P0):SHT_MOVE")
{
  field(OUT, "$(P):MoverOnOff PP")
  field(OOPT, "Every Time")
  field(DOPT, "Use CALC")
  field(INPA, "$(P0):SHT_CTRL")
  field(INPB, "$(P):MoverOnOffRBV")
  field(INPC, "1")
  field(INPD, "0x1")
  field(INPE, "0x3e")
  field(CALC, "(A = C) ? B|D : B&E")
  field(SCAN, "Passive")
}

