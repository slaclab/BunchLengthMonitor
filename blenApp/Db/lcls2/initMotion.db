# Initialization of motion devices on startup.

record(dfanout, "$(P):MOTIONINIT") {
  field(DESC, "Proc. Motion Devices on Boot")
#  field(PINI, "YES")
  field(VAL,  "1")
  field(OUTA, "$(P):0:SHT_CTRL.DISV PP")
  field(OUTB, "$(P):1:SHT_CTRL.DISV PP")
  field(OUTC, "$(P):FLT1_CTRL.DISV PP")
  field(OUTD, "$(P):FLT2_CTRL.DISV PP")
  field(OUTE, "$(P):FLT3_CTRL.DISV PP")
  field(OUTF, "$(P):FLT4_CTRL.DISV PP")
  field(OUTG, "$(P):MOTIONINIT2.PROC PP")
  field(ASG,  "Internal")

}

