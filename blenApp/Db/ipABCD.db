
#
record(calc, "$(P):$(SRC)A")
{
  field(INPA, "$(P):$(SRC)")
  field(INPB, "0xff")
  field(CALC, "(A&B)")
  field(SCAN, "10 second")
}

#
record(calc, "$(P):$(SRC)B")
{
  field(INPA, "$(P):$(SRC)")
  field(INPB, "0xff00")
  field(INPC, "8")
  field(CALC, "(A&B)>>C")
  field(SCAN, "10 second")
}

