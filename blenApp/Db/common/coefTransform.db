record(ai, "$(P):$(AMC):Coef$(COEF)LN"){
    field(DESC, "Coefficet input")
}

record(calcout, "$(P):$(AMC):Coef$(COEF)TF"){
    field(INPA, "$(P):$(AMC):Coef$(COEF)LN")
    field(CALC, "EXP(A)")
    field(OUT, "$(P):$(AMC):Coef$(COEF)")
    field(FLNK, "$(P):$(AMC):Coef$(COEF)")
    field(SCAN, "1 second")
}

record(calc, "$(P):$(AMC):Coef$(COEF)RBVTF"){
    field(INPA, "$(P):$(AMC):Coef$(COEF)RBV")
    field(CALC, "EXP(A)")
    field(SCAN, "1 second")
}
